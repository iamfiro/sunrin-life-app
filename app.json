{
    "expo": {
        "name": "sunrin-class",
        "slug": "sunrin-arlimi",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.sunrin.arlimi"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/WantedSans-ExtraBlack.ttf",
                        "./assets/fonts/WantedSans-Black.ttf",
                        "./assets/fonts/WantedSans-ExtraBold.ttf",
                        "./assets/fonts/WantedSans-Bold.ttf",
                        "./assets/fonts/WantedSans-SemiBold.ttf",
                        "./assets/fonts/WantedSans-Regular.ttf",
                        "./assets/fonts/WantedSans-Medium.ttf"
                    ]
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "91d97276-4d63-4c14-981e-f823f10b8b2a"
            }
        },
        "owner": "anojds"
    }
}